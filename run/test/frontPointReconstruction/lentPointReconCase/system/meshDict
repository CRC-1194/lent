FoamFile
{
    version	2;
    format	ascii;
    class	dictionary;
    location	"system";
    object	meshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// geometry (mandatory)
// fms is a native format
// other formats are also possible (stl, ftr, obj)
surfaceFile	"unit-box-domain.stl";

// global cell size (mandatory)
// it is the maximum cell size in the mesh
maxCellSize	0.1;

